	note = {Place: Singapore},
	keywords = {Age Factors, Humans, Parkinson Disease/diagnosis/*pathology/*therapy},
	pages = {65--74},
	file = {Parkinson’s disease a review.pdf:C\:\\Users\\bened\\Zotero\\storage\\R2XNQKA9\\Parkinson’s disease a review.pdf:application/pdf},
}
